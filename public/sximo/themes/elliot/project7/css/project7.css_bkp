div#left_wrapper{float:left; width:155px;margin-left:205px;min-height:1px;}

div#tree_toggle {
    background: url("images/elements/arrow_down_black.png") no-repeat scroll center bottom transparent;
    margin: -4px 0 10px;
    padding: 0 0 3px;
    width: 78px;
}
div#tree_toggle.hidden {background: none;}
div#tree_toggle div#tree_toggle_trigger {
	cursor:pointer;
    background:  #000000;
    color: #FFFFFF;
    font-size: 11px;
    height: 18px; 
    padding: 4px 0 0;
    text-align: center;
    width: 78px;
	font-style:italic;
	letter-spacing:0;
}
@media screen and (-webkit-min-device-pixel-ratio:0) {div#tree_toggle div#tree_toggle_trigger {    height: 30px;    padding: 11px 0 0;}}
body.ie8 div#tree_toggle div#tree_toggle_trigger {    height: 16px;    padding: 2px 0 0;}
div#tree_toggle.hidden div#tree_toggle_trigger { 
	background: none;
	color:#000;
	font-size:11px;
	width:78px;
	height:18px;
}
div#tree_toggle #tree_toggle_trigger:after{ content: " -"; }
div#tree_toggle.hidden #tree_toggle_trigger:after{ content: " +"; }

ul#tree li a {font-style:italic;}
ul#tree li.level1 {font-size:12px; line-height:36px;}
ul#tree li.level2 {font-size:12px;height:12px; padding:12px 0;}
ul#tree li.level3 {font-size:12px; line-height:18px;}
ul#tree li.level4 {font-size:12px; line-height:18px;}
ul#tree li.level5 {font-size:12px; line-height:18px;}

ul#tree li.active a, ul#tree li.expanded a{color:#000;}
ul#tree li.active li.expanded.hidden a {color:#000;}
ul#tree li.expanded.hidden a {color:#9B9B9B;}
ul#tree li.active,ul#tree li.expanded  {height:auto;}
ul#tree li.active a {text-decoration:underline;}
ul#tree li ul li a {
    color: #000 !important;
}
ul#tree li.active a {color:#000;}
ul#tree li.active ul{padding: 8px 0 0;}
ul#tree li.active.level3 ul,ul#tree li.active.level4 ul,ul#tree li.active.level5 ul {padding:  0;}
ul#tree li.active ul ul{padding: 0 0;}
ul#tree li.active li a {text-decoration:none;}
