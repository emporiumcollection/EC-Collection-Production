/*
--------------------------------------------------------------------------------
	HomeBrew Scrollbars
-------------------------------------------------------------------------------- 
*/

.scrollable_homebrew { display:block; width:100%; margin:0 0 20px; clear:both; }
.scrollable_homebrew .viewport { position:relative; width:95%; height:502px; overflow:hidden; }
.scrollable_homebrew .overview { position:absolute; left:0; top:0; }
.scrollable_homebrew .scrollbar {     background: none no-repeat scroll 0 0 #FFFFFF; float: right; position: relative; width: 3px; }
.scrollable_homebrew .track {    background: none no-repeat scroll 0 100% #CCCCCC; height: 100%; padding: 0; position: relative; width: 1px; }
.scrollable_homebrew .thumb {     
	background: none repeat scroll 0 0 #CCCCCC;
    border-bottom: 2px solid white;
    border-top: 2px solid white;
    cursor: pointer;
    display: none;
    height: 58px;
    left: -1px;
    overflow: hidden;
    position: absolute;
    top: 0;
    width: 3px;
}
.scrollable_homebrew .thumb .end { }
.scrollable_homebrew .disable { display:none; }




