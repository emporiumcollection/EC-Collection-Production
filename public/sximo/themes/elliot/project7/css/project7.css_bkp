/*
--------------------------------------------------------------------------------
	Top-Navigation & Suche
-------------------------------------------------------------------------------- 
*/

#top_nav_wrapper {     
    float: left;
    margin: 35px 0 0;
    position: relative;
    width: 100%;
    background: url("images/header/border.gif") top right no-repeat; 
    
}
#top_nav { float:left;  margin: 0 3px 0 0;}
#top_nav ul { display:block; float:left;}
#top_nav ul li { display:block; float:left; width:auto; */ display: inline; height:32px;padding: 0 0 3px;}
#top_nav ul li a {     
    display: block;
    float: left;
    font-size: 14px;
    font-style: italic;
    line-height: 23px;
    margin: 4px 9px 0;
    overflow: hidden;
    padding: 0 6px;
    text-align: center;
    text-decoration: none;
    width: auto;
}
#top_nav ul li:first-child a { margin-left:24px; }
#top_nav ul li a:hover,
#top_nav ul li a:active,
#top_nav ul li.selected a,
#top_nav ul li.open a { background:#000; color:#FFF; }
#top_nav ul li.open {      background: url("images/elements/arrow_down_black.png") no-repeat scroll center 27px transparent; }
#top_nav ul .top_nav_sep{float:left; display:block; padding: 8px 0 0;}
#top_nav ul li:last-child .top_nav_sep { display:none; }
/* subnav */

#top_nav ul li div.sub { position:absolute; display:none; float:left; top:30px; left:0; z-index:9999999; overflow:hidden; }
#top_nav ul li:hover div.sub { display:block; }
.js #top_nav ul li:hover div.sub { display:none; }

/* end: arrow positions */
#top_nav ul li div.sub_inner {    
	background: none repeat scroll 0 0 #FFFFFF;
    display: block;
    float: left;
    overflow: hidden;
	padding:40px;
	margin:-30px 0 0 -40px;
}
#top_nav ul li ul { float:left; display:block; padding:5px 0 0 30px; width:100%; }
#top_nav ul li li { float:left;  height:auto;padding:0; }
#top_nav ul li li a {     margin: 0 9px;    padding: 0;}
#top_nav ul li.open li a,
#top_nav ul li.selected li a,
#top_nav ul li a:hover li a {     background: none repeat scroll 0 0 #FFFFFF;    color: #000000; }
#top_nav ul li li a:hover { text-decoration:underline; }

#top_nav ul.fotograf_overview_list { float: left; width: 170px; }
#top_nav ul.fotograf_overview_list li { width:100%; }
#top_nav ul.fotograf_overview_list li a { text-align: left; }
#top_nav ul.fotograf_overview_list li:first-child a { margin: 0 9px;  }


#top_nav ul.fotograf_overview_list:last-child {
    width: 135px;
}


#top_nav ul.fotograf_overview_list_ap { float: left; width: 300px; }
#top_nav ul.fotograf_overview_list_ap li { width:100%; }
#top_nav ul.fotograf_overview_list_ap li a { text-align: left; }
#top_nav ul.fotograf_overview_list_ap li:first-child a { margin: 0 9px;  }


