ul#tree li.hidden.expanded.level5 {padding:  0;}

ul#tree li.end a:after { content: ""!important; }
ul#tree li.hasSubCats a:after { content: " +"; }
ul#tree li.hasSubCats.expanded a:after { content: " -"; }
