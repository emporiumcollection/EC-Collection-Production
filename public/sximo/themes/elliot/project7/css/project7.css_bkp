/* --- dropdown ------------- */
.drop li {
    list-style: none;
    padding: 0;
}

.dropDown{
    position:relative;
    float:left;
    cursor:pointer;
    padding-right: 10px;
    min-width: 120px;
    cursor: pointer;
}


.dropDown ul{
    position: absolute;
    top: -4px;
    display: none;
    left: -1px;
    margin: 0;
    z-index: 20;
    border: 1px solid #a4a9aa;
}

.dropDown ul a{
    text-decoration:none;
    display:block;
    padding: 3px 5px;
    border-top: 1px solid #fff;
    border-bottom: 1px solid #b4b8b8;
}


